{{#partial "body"}}
<div class="container">
<h4>Guardaropas</h4>
<form>
<div class="form-group col-md-4">
      <label for="prendas">Prendas:</label>
      <select id="guardaropasA" name="guardaropasA" class="form-control">
        <option selected>Choose...</option>
         {{#each prendasA}}  
        <option value="{{this}}">{{this}}</option>
        {{/each}}
      </select>
</div>

<div class="form-group col-md-4">
      <label for="guardaropasB">Prendas:</label>
      <select id="guardaropasB" name="guardaropasB" class="form-control">
        <option selected>Choose...</option>
         {{#each prendasB}}  
        <option value="{{this}}">{{this}}</option>
        {{/each}}
      </select>
</div>
</form>
</div>
{{/partial}}
{{>layout.hbs}}